<!DOCTYPE html>
<html lang="en">
    <%- include('../partials/head.ejs') %>
<body>
    <%- include('../partials/nav.ejs') %>
    <div class="blogs">
        <h1>Blogs</h1>
        <% blogs.forEach((content)=>{ %>
            <div class="cont">
                <% blogURL = `/blogs/${content._id}` %>
                <h2 class="blog-cont">
                    <a href="<%= blogURL %>"><%= content.title %></a>
                </h2>
                <h3 class="blog-cont">
                    <a href="<%= blogURL %>"><%= content.snippet %></a>
                </h3>
            <% }) %>
        </div>
    </div>


</body>
</html>